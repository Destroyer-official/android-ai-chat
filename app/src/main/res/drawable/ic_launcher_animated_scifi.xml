<?xml version="1.0" encoding="utf-8"?>
<animated-vector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt">
    <aapt:attr name="android:drawable">
        <vector
            android:width="108dp"
            android:height="108dp"
            android:viewportWidth="108"
            android:viewportHeight="108">
            
            <!-- Background gradient -->
            <path
                android:fillColor="#0F172A"
                android:pathData="M0,0h108v108h-108z" />
            
            <!-- Outer rotating ring -->
            <group
                android:name="outerRing"
                android:pivotX="54"
                android:pivotY="54">
                <path
                    android:strokeColor="#6366F1"
                    android:strokeWidth="4"
                    android:fillColor="@android:color/transparent"
                    android:pathData="M54,8A46,46 0 0,1 100,54A46,46 0 0,1 54,100A46,46 0 0,1 8,54A46,46 0 0,1 54,8z" />
                <!-- Outer ring dots -->
                <path
                    android:fillColor="#6366F1"
                    android:pathData="M54,4m-4,0a4,4 0,1 1,8 0a4,4 0,1 1,-8 0" />
                <path
                    android:fillColor="#6366F1"
                    android:pathData="M104,54m-4,0a4,4 0,1 1,8 0a4,4 0,1 1,-8 0" />
                <path
                    android:fillColor="#6366F1"
                    android:pathData="M54,104m-4,0a4,4 0,1 1,8 0a4,4 0,1 1,-8 0" />
                <path
                    android:fillColor="#6366F1"
                    android:pathData="M4,54m-4,0a4,4 0,1 1,8 0a4,4 0,1 1,-8 0" />
            </group>
            
            <!-- Middle rotating ring -->
            <group
                android:name="middleRing"
                android:pivotX="54"
                android:pivotY="54">
                <path
                    android:strokeColor="#F59E0B"
                    android:strokeWidth="3"
                    android:fillColor="@android:color/transparent"
                    android:pathData="M54,18A36,36 0 0,1 90,54A36,36 0 0,1 54,90A36,36 0 0,1 18,54A36,36 0 0,1 54,18z" />
                <!-- Middle ring dots -->
                <path
                    android:fillColor="#F59E0B"
                    android:pathData="M54,15m-3,0a3,3 0,1 1,6 0a3,3 0,1 1,-6 0" />
                <path
                    android:fillColor="#F59E0B"
                    android:pathData="M93,54m-3,0a3,3 0,1 1,6 0a3,3 0,1 1,-6 0" />
                <path
                    android:fillColor="#F59E0B"
                    android:pathData="M54,93m-3,0a3,3 0,1 1,6 0a3,3 0,1 1,-6 0" />
                <path
                    android:fillColor="#F59E0B"
                    android:pathData="M15,54m-3,0a3,3 0,1 1,6 0a3,3 0,1 1,-6 0" />
            </group>
            
            <!-- Inner core background -->
            <path
                android:fillColor="#1E293B"
                android:pathData="M54,28A26,26 0 0,1 80,54A26,26 0 0,1 54,80A26,26 0 0,1 28,54A26,26 0 0,1 54,28z" />
            
            <!-- Neural network -->
            <group android:name="neuralNetwork">
                <!-- Central pulsing node -->
                <path
                    android:name="centerNode"
                    android:fillColor="#10B981"
                    android:pathData="M54,54m-6,0a6,6 0,1 1,12 0a6,6 0,1 1,-12 0" />
                
                <!-- Surrounding nodes -->
                <path
                    android:name="topNode"
                    android:fillColor="#10B981"
                    android:pathData="M54,38m-3,0a3,3 0,1 1,6 0a3,3 0,1 1,-6 0" />
                <path
                    android:name="rightNode"
                    android:fillColor="#10B981"
                    android:pathData="M70,54m-3,0a3,3 0,1 1,6 0a3,3 0,1 1,-6 0" />
                <path
                    android:name="bottomNode"
                    android:fillColor="#10B981"
                    android:pathData="M54,70m-3,0a3,3 0,1 1,6 0a3,3 0,1 1,-6 0" />
                <path
                    android:name="leftNode"
                    android:fillColor="#10B981"
                    android:pathData="M38,54m-3,0a3,3 0,1 1,6 0a3,3 0,1 1,-6 0" />
                
                <!-- Connection lines -->
                <path
                    android:name="connections"
                    android:strokeColor="#10B981"
                    android:strokeWidth="2"
                    android:pathData="M54,48 L54,41 M60,54 L67,54 M54,60 L54,67 M48,54 L41,54" />
            </group>
        </vector>
    </aapt:attr>
    
    <!-- Outer ring slow rotation -->
    <target android:name="outerRing">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="rotation"
                android:duration="8000"
                android:valueFrom="0"
                android:valueTo="360"
                android:repeatCount="infinite"
                android:interpolator="@android:anim/linear_interpolator" />
        </aapt:attr>
    </target>
    
    <!-- Middle ring fast counter-rotation -->
    <target android:name="middleRing">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="rotation"
                android:duration="5000"
                android:valueFrom="360"
                android:valueTo="0"
                android:repeatCount="infinite"
                android:interpolator="@android:anim/linear_interpolator" />
        </aapt:attr>
    </target>
    
    <!-- Center node pulsing -->
    <target android:name="centerNode">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:propertyName="scaleX"
                    android:duration="2000"
                    android:valueFrom="1.0"
                    android:valueTo="1.4"
                    android:repeatCount="infinite"
                    android:repeatMode="reverse"
                    android:interpolator="@android:anim/accelerate_decelerate_interpolator" />
                <objectAnimator
                    android:propertyName="scaleY"
                    android:duration="2000"
                    android:valueFrom="1.0"
                    android:valueTo="1.4"
                    android:repeatCount="infinite"
                    android:repeatMode="reverse"
                    android:interpolator="@android:anim/accelerate_decelerate_interpolator" />
            </set>
        </aapt:attr>
    </target>
    
    <!-- Neural network alpha pulsing -->
    <target android:name="neuralNetwork">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="alpha"
                android:duration="3000"
                android:valueFrom="0.7"
                android:valueTo="1.0"
                android:repeatCount="infinite"
                android:repeatMode="reverse"
                android:interpolator="@android:anim/accelerate_decelerate_interpolator" />
        </aapt:attr>
    </target>
    
    <!-- Connection lines pulsing -->
    <target android:name="connections">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="alpha"
                android:duration="1500"
                android:valueFrom="0.3"
                android:valueTo="1.0"
                android:repeatCount="infinite"
                android:repeatMode="reverse"
                android:interpolator="@android:anim/accelerate_decelerate_interpolator" />
        </aapt:attr>
    </target>
</animated-vector>